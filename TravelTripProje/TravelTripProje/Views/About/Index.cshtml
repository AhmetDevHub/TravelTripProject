@using TravelTripProje.Models.Sınıflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
<div class="wrap">
    <div class="about">
        <h3>Hakkımızda</h3>
        @foreach (var x in Model)
        {
            <img style="width:9000px; height:400px;" src="@x.FotoUrl" alt="" />

            <p>@x.Aciklama</p>
        }

    </div>
</div>
</div>

